<h3>Vod Overview</h3>

<div class="search-container flex-container flex-row">
    <Search::ModelSearch @model={{model}} @updateSearch={{updateSearch}} />
</div>

<div class="flex-container flex-row">
    {{#each @model as |vod|}}
        <ModelCard::VodCard @route="vods.vod" @vod={{vod}} />
    {{/each}}
</div>
